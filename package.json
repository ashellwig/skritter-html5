{
  "name": "skritter-html5",
  "title": "Skritter",
  "description": "Learn to write Chinese and Japanese characters.",
  "author": {
    "name": "Inkren, Inc",
    "email": "team@skritter.com",
    "url": "https://html5.skritter.com"
  },
  "version": "2.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/skritter/skritter-html5.git"
  },
  "cordova": {
    "id": "com.inkren.skritter",
    "name": "Skritter"
  },
  "scripts": {
    "android-build-chinese": "node ./bin/cordova-build-android --lang='zh'",
    "android-build-japanese": "node ./bin/cordova-build-android --lang='ja'",
    "android-install": "node ./bin/cordova-install-android",
    "android-run-chinese": "node ./bin/cordova-run-android --lang='zh'",
    "android-run-japanese": "node ./bin/cordova-run-android --lang='ja'",
    "install": "./node_modules/.bin/bower install",
    "ios-build-chinese": "node ./bin/cordova-build-android --lang='zh'",
    "ios-build-japanese": "node ./bin/cordova-build-android --lang='ja'",
    "ios-install": "node ./bin/cordova-install-ios",
    "ios-run-chinese": "node ./bin/cordova-run-ios --lang='zh'",
    "ios-run-japanese": "node ./bin/cordova-run-ios --lang='ja'",
    "postversion": "./node_modules/.bin/bower update",
    "start": "node ./bin/start",
    "test": "node ./bin/test",
    "web-build": "./node_modules/.bin/brunch build --production",
    "web-watch": "./node_modules/.bin/brunch watch --server"
  },
  "dependencies": {
    "bower": "~1.7.0",
    "brunch": "~2.1.0",
    "chai": "~3.4.0",
    "cordova": "~5.4.0",
    "lodash": "~3.10.0",
    "minimist": "~1.2.0",
    "mocha": "~2.3.0",
    "mocha-phantomjs": "~4.0.0",
    "shelljs": "~0.5.0",
    "auto-reload-brunch": "~1.8.0",
    "clean-css-brunch": "~1.8.0",
    "css-brunch": "~1.7.0",
    "jade-brunch": "~1.8.0",
    "javascript-brunch": "~1.8.0",
    "replace-brunch": "~1.0.0",
    "sass-brunch": "~1.9.0",
    "uglify-js-brunch": "~1.7.0"
  }
}
