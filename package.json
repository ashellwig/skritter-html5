{
  "name": "skritter-html5",
  "title": "Skritter",
  "description": "Learn to write Chinese and Japanese characters.",
  "author": {
    "name": "Inkren, Inc",
    "email": "team@skritter.com",
    "url": "https://html5.skritter.com"
  },
  "version": "2.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/skritter/skritter-html5.git"
  },
  "cordova": {
    "id": "com.inkren.skritter",
    "name": "Skritter"
  },
  "scripts": {
    "install": "./node_modules/.bin/bower install",
    "postversion": "./node_modules/.bin/bower update",
    "start": "./node_modules/.bin/brunch watch --server",
    "test": "./node_modules/.bin/brunch build && ./node_modules/.bin/mocha-phantomjs ./test/index.html",
    "web-build": "./node_modules/.bin/brunch build --production",
    "web-watch": "./node_modules/.bin/brunch watch --server"
  },
  "dependencies": {
    "bower": "1.6.5",
    "brunch": "1.8.5",
    "chai": "3.4.1",
    "cordova": "5.4.0",
    "mocha": "2.3.4",
    "mocha-phantomjs": "4.0.1",
    "auto-reload-brunch": "1.8.1",
    "clean-css-brunch": "1.8.0",
    "css-brunch": "1.7.0",
    "jade-brunch": "1.8.2",
    "javascript-brunch": "1.8.0",
    "replace-brunch": "0.1.0",
    "sass-brunch": "1.9.1",
    "uglify-js-brunch": "1.7.8"
  }
}
